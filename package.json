    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.0.1",
    "@types/testing-library__jest-dom": "5.0.2",
    "@types/webpack-env": "1.15.1",
    "antd": "4.4.2",
    "axios": "0.21.1",
    "chart.js": "2.9.3",
    "color": "3.1.2",
    "craco": "0.0.3",
    "craco-less": "1.17.0",
    "cross-env": "7.0.2",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "fontfaceobserver": "2.1.0",
    "husky": "4.2.3",
    "i": "0.3.6",
    "i18next": "19.3.4",
    "i18next-browser-languagedetector": "4.0.2",
    "jest-styled-components": "7.0.2",
    "jsdoc": "3.6.7",
    "lint-staged": "10.0.8",
    "moment": "2.27.0",
    "moment-jalaali": "0.9.2",
    "node-plop": "0.25.0",
    "npm": "7.11.2",
    "plop": "2.6.0",
    "prettier": "2.0.1",
    "react": "16.13.0",
    "react-app-polyfill": "1.0.6",
    "react-chartjs-2": "2.9.0",
    "react-dom": "16.13.0",
    "react-helmet-async": "1.0.4",
    "react-i18next": "11.3.4",
    "react-lz-editor": "0.12.1",
    "react-multi-carousel": "2.6.2",
    "react-quill": "1.3.5",
    "react-recaptcha": "2.3.10",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.4.1",
    "react-show-more-text": "1.4.4",
    "react-test-renderer": "16.13.0",
    "redux-injectors": "1.3.0",
    "redux-saga": "1.1.3",
    "sanitize.css": "11.0.0",
    "serve": "11.3.0",
    "shelljs": "0.8.3",
    "styled-components": "5.0.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0",
    "swiper": "6.5.9",
    "ts-node": "8.8.2",
    "typescript": "3.8.2"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "test:generators": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/testing/test-generators.ts",
    "start:prod": "yarn build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn eslint -- src",
    "lint:fix": "yarn eslint -- --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "cross-env TS_NODE_PROJECT='./internals/ts-node.tsconfig.json' plop --plopfile internals/generators/plopfile.ts",
    "component": "yarn run generate component",
    "page": "yarn run generate container",
    "prettify": "prettier --write"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "npm": ">=6.4.1",
    "node": ">=10.13.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "npm run eslint -- --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run checkTs && lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  }
}
